<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  >
	  <LinearLayout
		  xmlns:android="http://schemas.android.com/apk/res/android"
		  android:layout_width="match_parent"
		  android:layout_height="match_parent"
		  android:orientation="vertical"
	  >
		<TextView 
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="显示选项"
			  android:layout_margin="10dip"
		 />
		  <!--<View 
			  android:layout_width="match_parent"
			  android:layout_height="1dip"
			  android:background="@drawable/divider_horizontal_grey"
			  />
		-->
		<RelativeLayout
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:layout_marginLeft="20dip"
		  android:layout_marginRight="20dip"
		  >
		  <!--<ImageView
			  android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:src="@drawable/call_book" 
			  android:id="@+id/v_see_only_phone"
			  android:layout_marginRight="10dip"
			  android:layout_centerVertical="true"
			  />
		  -->
		  <LinearLayout
		  	android:layout_width="match_parent"
		  	android:layout_height="wrap_content"
		 	android:orientation="vertical"
		  >
		  <TextView 
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="@string/preference_see_only_phone"
			  android:layout_marginRight="20dip"
			  android:layout_centerVertical="true"
		  />
		  <TextView 
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="@string/preference_see_only_phone"
			  android:layout_marginRight="20dip"
			  android:layout_centerVertical="true"
		  />
		  </LinearLayout>			 
		  <CheckBox 
			  android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:checked="true"
			  android:layout_alignParentRight="true"	
		      android:id="@+id/key_see_only_phone"	  
			  /> 
		</RelativeLayout>
		<TextView 
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="选择要显示的联系人"
			  android:layout_margin="10dip"
			  android:layout_centerVertical="true"
		/>
		<RelativeLayout
		  android:layout_width="match_parent"
		  android:layout_height="wrap_content"
		  android:layout_marginLeft="20dip"
		  android:layout_marginRight="20dip"
		  ><!--
		  <ImageView
			  android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:src="@drawable/sim_book" 
			  android:id="@+id/v_see_only_sim"
			  android:layout_marginRight="10dip"
			  android:layout_centerVertical="true"
			  />
		  -->
		  <LinearLayout
		  	android:layout_width="match_parent"
		  	android:layout_height="wrap_content"
		 	android:orientation="vertical"
		  >
		  <TextView 
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="@string/preference_see_only_sim"
			  android:layout_marginRight="20dip"
			  android:layout_centerVertical="true"
		  />
		  <TextView 
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="@string/see_only_sim"
			  android:layout_marginRight="20dip"
			  android:layout_centerVertical="true"
		  />
		  </LinearLayout>				 
		  <CheckBox 
			  android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:checked="true"
			  android:layout_alignParentRight="true"
		      android:id="@+id/key_see_only_sim"		  
			  /> 
		</RelativeLayout>
		<View 
			  android:layout_width="match_parent"
			  android:layout_height="1dip"
			  android:background="@drawable/divider_vertical_dark"
			  />
		<RelativeLayout
		  android:layout_width="match_parent"
		  android:layout_height="wrap_content"
		  android:layout_marginLeft="20dip"
		  android:layout_marginRight="20dip"
		  >
		  <!--<ImageView
			  android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:src="@drawable/web_book" 
			  android:layout_marginRight="10dip"
			  android:layout_centerVertical="true"
			  android:id="@+id/v_see_only_net"		  
			  />
		  -->
		  <LinearLayout
		  	android:layout_width="match_parent"
		  	android:layout_height="wrap_content"
		 	android:orientation="vertical"
		  >
		  <TextView 
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="@string/preference_see_only_net"
			  android:layout_marginRight="20dip"
			  android:layout_centerVertical="true"
		  />
		  <TextView 
		  	android:id="@+id/see_only_net_tv"
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="@string/preference_see_only_net"
			  android:layout_marginRight="20dip"
			  android:layout_centerVertical="true"
		  />
		  </LinearLayout>			 
		  <CheckBox 
			  android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:checked="true"
			  android:layout_alignParentRight="true"	
		      android:id="@+id/key_see_only_net"	
			  /> 
		  </RelativeLayout>
		</LinearLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:layout_alignParentBottom="true"
			>
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="60dip"
        		android:layout_centerInParent="true"
				android:orientation="horizontal"
			>
			    <Button 
			    	android:layout_width="100dip"
			    	android:layout_height="wrap_content"
			    	android:id="@+id/btn_setting_done"
			    	android:text="@string/menu_done"
			    	android:background="@drawable/btn_see_opt" 
			    	android:textColor="@color/textColorIconOverlay"
    				android:layout_margin="5dip"
    				 android:layout_centerVertical="true"
			    	/>
			    <Button 
			    	android:layout_width="100dip"
			    	android:layout_height="wrap_content"
			    	android:id="@+id/btn_setting_doNotSave"
			    	android:text="@string/menu_doNotSave"
			    	android:background="@drawable/btn_see_opt" 
			    	android:textColor="@color/textColorIconOverlay"
    				android:layout_margin="5dip"
    				 android:layout_centerVertical="true"
			    	/>			
			</LinearLayout>
	</RelativeLayout>
 </RelativeLayout>